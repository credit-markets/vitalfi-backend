{
  "version": "0.1.0",
  "name": "vitalfi_vault",
  "instructions": [],
  "accounts": [
    {
      "name": "vault",
      "type": {
        "kind": "struct",
        "fields": [
          { "name": "version", "type": "u16" },
          { "name": "authority", "type": "publicKey" },
          { "name": "vaultId", "type": "u64" },
          { "name": "assetMint", "type": "publicKey" },
          { "name": "vaultToken", "type": "publicKey" },
          { "name": "cap", "type": "u64" },
          { "name": "targetApyBps", "type": "u32" },
          { "name": "fundingEndTs", "type": "i64" },
          { "name": "maturityTs", "type": "i64" },
          { "name": "minDeposit", "type": "u64" },
          {
            "name": "status",
            "type": {
              "defined": "VaultStatus"
            }
          },
          { "name": "totalDeposited", "type": "u64" },
          { "name": "totalClaimed", "type": "u64" },
          { "name": "payoutNum", "type": "u128" },
          { "name": "payoutDen", "type": "u128" },
          { "name": "bump", "type": "u8" }
        ]
      }
    },
    {
      "name": "position",
      "type": {
        "kind": "struct",
        "fields": [
          { "name": "vault", "type": "publicKey" },
          { "name": "owner", "type": "publicKey" },
          { "name": "deposited", "type": "u64" },
          { "name": "claimed", "type": "u64" },
          { "name": "bump", "type": "u8" }
        ]
      }
    }
  ],
  "types": [
    {
      "name": "VaultStatus",
      "type": {
        "kind": "enum",
        "variants": [
          { "name": "Funding" },
          { "name": "Active" },
          { "name": "Canceled" },
          { "name": "Matured" },
          { "name": "Closed" }
        ]
      }
    }
  ]
}
